{
  "family": "task-ms-family-budget",
  "networkMode": "awsvpc",
  "cpu": 256,
  "memory": 512,
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "ms-family-budget",
      "container-name": "ms-family-budget",
      "image": "${{ vars.ECR_REPOSITORY_URL }}:${{ github.sha }}",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}
