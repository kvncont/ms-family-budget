{
  "family": "ms-family-budget",
  "containerDefinitions": [
    {
      "name": "ms-family-budget",
      "container-name": "ms-family-budget",
      "image": "${{ vars.ECR_REPOSITORY_URL }}:${{ github.sha }}",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": "80",
          "hostPort": "80"
        }
      ],
      "requiresCompatibilities": [
          "FARGATE"
      ]
    }
  ]
}
