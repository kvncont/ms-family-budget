{
  "family": "task-ms-family-budget",
  "executionRoleArn": "arn:aws:iam::786243266451:role/MsFamilyBudgetRole",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "ms-family-budget",
      "container-name": "ms-family-budget",
      "image": "${{ vars.ECR_REPOSITORY_URL }}",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ecs-logs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "tags": [
        {
            "key": "App",
            "value": "ms-family-budget"
        },
        {
            "key": "ManagedBy",
            "value": "gh-actions"
        },
        {
            "key": "GitRepoURL",
            "value": "https://github.com/kvncont/ms-family-budget-test"
        }
    ]
}
